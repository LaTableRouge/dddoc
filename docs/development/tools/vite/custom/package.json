{
  "name": "yourproject",
  "version": "2.0.0",
  "description": "## VHosts",
  "main": "vite.config.js",
  "scripts": {
    "watch": "vite",
    "prod": "vite build",
    "build": "npm run prod --chore=all",
    "lint:scss": "npm run prod --chore=lint:scss",
    "lint:js": "npm run prod --chore=lint:js",
    "lint:php": "npm run prod --chore=lint:php",
    "prettier:scss": "npm run prod --chore=prettier:scss",
    "prettier:js": "npm run prod --chore=prettier:js"
  },
  "lint-staged": {
    "assets/scss/**/*.scss": [
      "npx stylelint --config .stylelintrc.json --allow-empty-input --ignore-path .stylelintignore --fix",
      "npx prettier --config .prettierrc.js --ignore-path .prettierignore --write",
      "npx stylelint --config .stylelintrc.json --allow-empty-input --ignore-path .stylelintignore --fix"
    ],
    "assets/js/**/*.js": [
      "npx prettier --config .prettierrc.js --ignore-path .prettierignore --write",
      "npx eslint --config .eslintrc.js --ignore-path .eslintignore --fix"
    ]
  },
  "devDependencies": {
    "@mlnop/string-replace": "^1.0.5",
    "@rollup/plugin-terser": "^0.4.3",
    "autoprefixer": "^10.4.15",
    "browserslist": "^4.21.10",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.0.2",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.10.3",
    "stylelint-config-property-sort-order-smacss": "^9.1.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "validate-branch-name": "^1.3.0",
    "vite": "^4.4.9",
    "vite-plugin-run": "^0.4.1",
    "vite-plugin-static-copy": "^0.17.0"
  },
  "keywords": [],
  "author": "Development Team",
  "license": "(╯°□°）╯︵ ┻━┻",
  "dependencies": {}
}
